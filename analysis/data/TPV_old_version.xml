<?xml version="1.0" encoding="utf-8"?>
	<Schema name="Mondrian">
		<Cube name="TPV">
			<Table name="TPV"/>
			<Dimension foreignKey="TIME" name="TimeDim">
				<Hierarchy allMemberName="AllTime" hasAll="true" primaryKey="TIME">
					<Table name="TIME"/>
					<Level column="YEAR" name="year"/>
					<Level column="QUARTER" name="quarter"/>
					<Level column="MONTH" name="month"/>
					<Level column="XUN" name="xun"/>
					<Level column="DAY" name="day"/>
					<Level column="HOUR" name="hour"/>
				</Hierarchy>
			</Dimension>
			<Dimension foreignKey="LONGITUDE" name="LongitudeDim">
				<Hierarchy allMemberName="AllLongitude" hasAll="true" primaryKey="LONGITUDE">
					<Table name="LONGITUDE"/>
					<Level column="LON_10" name="lon_10"/>
					<Level column="LON_2" name="lon_2"/>
					<Level column="LON_1" name="lon_1"/>
					<Level column="LON_0_2" name="lon_0_2"/>
				</Hierarchy>
			</Dimension>
			<Measure aggregator="avg" column="TEMPERATURE" name="temperature"/>
			<Measure aggregator="avg" column="PRESSURE" name="pressure"/>
			<Measure aggregator="avg" column="WIND_U" name="wind_u"/>
			<Measure aggregator="avg" column="WIND_V" name="wind_v"/>
			<Measure aggregator="avg" column="WIND_W" name="wind_w"/>
		</Cube>
	</Schema>
